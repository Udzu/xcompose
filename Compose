###########################################################
# International Phonetic Alphabet (and other linguistics) #
###########################################################

# Also see brackets and combining diacritics sections

<Multi_key> <b> <h>                 : "Œ≤"  U03B2   # GREEK SMALL LETTER BETA
<Multi_key> <g> <h>                 : "…£"  U0263   # LATIN SMALL LETTER GAMMA
<Multi_key> <j> <h>                 : " ù"  U029D   # LATIN SMALL LETTER J WITH CROSSED-TAIL
<Multi_key> <l> <h>                 : "…¨"  U026C   # LATIN SMALL LETTER L WITH BELT
<Multi_key> <p> <h>                 : "…∏"  U0278   # LATIN SMALL LETTER PHI
<Multi_key> <q> <h>                 : "œá"  U03C7   # GREEK SMALL LETTER CHI (alternatively xh)
<Multi_key> <s> <h>                 : " É"  U0283   # LATIN SMALL LETTER ESH
<Multi_key> <x> <h>                 : "œá"  U03C7   # GREEK SMALL LETTER CHI
<Multi_key> <z> <h>                 : " í"  U0292   # LATIN SMALL LETTER EZH
<Multi_key> <g> <n>                 : "…≤"  U0272   # LATIN SMALL LETTER N WITH LEFT HOOK
<Multi_key> <m> <f>                 : "…±"  U0271   # LATIN SMALL LETTER M WITH HOOK
<Multi_key> <m> <g>                 : "…±"  U0271   # LATIN SMALL LETTER M WITH HOOK
<Multi_key> <n> <g>                 : "≈ã"  U014B   # LATIN SMALL LETTER ENG
<Multi_key> <r> <d>                 : "…æ"  U027E   # LATIN SMALL LETTER R WITH FISHHOOK
<Multi_key> <g> <w>                 : "…∞"  U0270   # LATIN SMALL LETTER TURNED M WITH LONG LEG
<Multi_key> <v> <w>                 : " ã"  U028B   # LATIN SMALL LETTER V WITH HOOK

<Multi_key> <a> <h>                 : "…ë"  U0251   # LATIN SMALL LETTER ALPHA
<Multi_key> <e> <h>                 : "…õ"  U025B   # LATIN SMALL LETTER OPEN E
<Multi_key> <i> <h>                 : "…™"  U026A   # LATIN LETTER SMALL CAPITAL I
<Multi_key> <u> <h>                 : " ä"  U028A   # LATIN SMALL LETTER UPSILON
<Multi_key> <w> <h>                 : "…Ø"  U026F   # LATIN SMALL LETTER TURNED M
<Multi_key> <y> <h>                 : " è"  U028F   # LATIN LETTER SMALL CAPITAL Y
<Multi_key> <e> <r>                 : "…ö"  U025A   # LATIN SMALL LETTER SCHWA WITH HOOK

<Multi_key> <b> <B>                 : " ô"  U0299   # LATIN LETTER SMALL CAPITAL B
<Multi_key> <B> <B>                 : " ô"  U0299   # LATIN LETTER SMALL CAPITAL B
<Multi_key> <G> <G>                 : "…¢"  U0262   # LATIN LETTER SMALL CAPITAL G
<Multi_key> <i> <I>                 : "…™"  U026A   # LATIN LETTER SMALL CAPITAL I
<Multi_key> <I> <I>                 : "…™"  U026A   # LATIN LETTER SMALL CAPITAL I
<Multi_key> <h> <H>                 : " ú"  U029C   # LATIN LETTER SMALL CAPITAL H
<Multi_key> <H> <H>                 : " ú"  U029C   # LATIN LETTER SMALL CAPITAL H
<Multi_key> <l> <L>                 : " ü"  U029F   # LATIN LETTER SMALL CAPITAL L
<Multi_key> <n> <N>                 : "…¥"  U0274   # LATIN LETTER SMALL CAPITAL N
<Multi_key> <r> <R>                 : " Ä"  U0280   # LATIN LETTER SMALL CAPITAL R
<Multi_key> <y> <Y>                 : " è"  U028F   # LATIN LETTER SMALL CAPITAL Y
