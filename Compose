<Multi_key> <m> <a> <a> : "¬∑-"     # MORSE CODE A
<Multi_key> <m> <a> <b> : "-¬∑¬∑¬∑"   # MORSE CODE B
<Multi_key> <m> <a> <c> : "-¬∑-¬∑"   # MORSE CODE C
<Multi_key> <m> <a> <d> : "-¬∑¬∑"    # MORSE CODE D
<Multi_key> <m> <a> <e> : "¬∑" U00B7  # MORSE CODE E (MIDDLE DOT)
<Multi_key> <m> <a> <f> : "¬∑¬∑-¬∑"   # MORSE CODE F
<Multi_key> <m> <a> <g> : "--¬∑"    # MORSE CODE G
<Multi_key> <m> <a> <h> : "¬∑¬∑¬∑¬∑"   # MORSE CODE H
<Multi_key> <m> <a> <i> : "¬∑¬∑"     # MORSE CODE I
<Multi_key> <m> <a> <j> : "¬∑---"   # MORSE CODE J
<Multi_key> <m> <a> <k> : "-¬∑-"    # MORSE CODE K
<Multi_key> <m> <a> <l> : "¬∑-¬∑¬∑"   # MORSE CODE L
<Multi_key> <m> <a> <m> : "--"     # MORSE CODE M
<Multi_key> <m> <a> <n> : "-¬∑"     # MORSE CODE N
<Multi_key> <m> <a> <o> : "---"    # MORSE CODE O
<Multi_key> <m> <a> <p> : "¬∑--¬∑"   # MORSE CODE P
<Multi_key> <m> <a> <q> : "--¬∑-"   # MORSE CODE Q
<Multi_key> <m> <a> <r> : "¬∑-¬∑"    # MORSE CODE R
<Multi_key> <m> <a> <s> : "¬∑¬∑¬∑"    # MORSE CODE S
<Multi_key> <m> <a> <t> : "-" U002D  # MORSE CODE T (HYPHEN-MINUS)
<Multi_key> <m> <a> <u> : "¬∑¬∑-"    # MORSE CODE U
<Multi_key> <m> <a> <v> : "¬∑¬∑¬∑-"   # MORSE CODE V
<Multi_key> <m> <a> <w> : "¬∑--"    # MORSE CODE W
<Multi_key> <m> <a> <x> : "-¬∑¬∑-"   # MORSE CODE X
<Multi_key> <m> <a> <y> : "-¬∑--"   # MORSE CODE Y
<Multi_key> <m> <a> <z> : "--¬∑¬∑"   # MORSE CODE Z

