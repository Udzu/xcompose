<Multi_key> <e> <e>                 : "…ô"  U0259   # LATIN SMALL LETTER SCHWA
<Multi_key> <r> <r>                 : "…π"  U0279   # LATIN SMALL LETTER TURNED R

<Multi_key> <a> <v>                 : "…ê"  U0250   # LATIN SMALL LETTER TURNED A
<Multi_key> <e> <v>                 : "…ô"  U0259   # LATIN SMALL LETTER SCHWA
<Multi_key> <h> <v>                 : "…•"  U0265   # LATIN SMALL LETTER TURNED H
<Multi_key> <m> <v>                 : "…Ø"  U026F   # LATIN SMALL LETTER TURNED M
<Multi_key> <r> <v>                 : "…π"  U0279   # LATIN SMALL LETTER TURNED R
<Multi_key> <R> <v>                 : " Å"  U0281   # LATIN LETTER SMALL CAPITAL INVERTED R
<Multi_key> <v> <v>                 : " å"  U028C   # LATIN SMALL LETTER TURNED V
<Multi_key> <W> <v>                 : " ç"  U028D   # LATIN SMALL LETTER TURNED W
<Multi_key> <y> <v>                 : " é"  U028E   # LATIN SMALL LETTER TURNED Y

<Multi_key> <a> <less>              : "…í"  U0252   # LATIN SMALL LETTER TURNED ALPHA (close enough)
<Multi_key> <e> <less>              : "…ò"  U0258   # LATIN SMALL LETTER REVERSED E (conflicts with ƒõ, use ve)
<Multi_key> <question> <less> <period> : " ï"  U0295   # LATIN LETTER PHARYNGEAL VOICED FRICATIVE
<Multi_key> <question> <less> <minus> : " ¢"  U02A2   # LATIN LETTER REVERSED GLOTTAL STOP WITH STROKE

<Multi_key> <d> <l>                 : "…ñ"  U0256   # LATIN SMALL LETTER D WITH TAIL
<Multi_key> <n> <l>                 : "…≥"  U0273   # LATIN SMALL LETTER N WITH RETROFLEX HOOK
<Multi_key> <s> <l>                 : " Ç"  U0282   # LATIN SMALL LETTER S WITH HOOK
<Multi_key> <z> <l>                 : " ê"  U0290   # LATIN SMALL LETTER Z WITH RETROFLEX HOOK
<Multi_key> <r> <l> <Return>        : "…Ω"  U027D   # LATIN SMALL LETTER R WITH TAIL
<Multi_key> <r> <l> <v>             : "…ª"  U027B   # LATIN SMALL LETTER TURNED R WITH HOOK
<Multi_key> <c> <at>                : "…ï"  U0255   # LATIN SMALL LETTER C WITH CURL
<Multi_key> <z> <at>                : " ë"  U0291   # LATIN SMALL LETTER Z WITH CURL
<Multi_key> <h> <minus>             : "ƒß"  U0127   # LATIN SMALL LETTER H WITH STROKE
<Multi_key> <j> <minus>             : "…ü"  U025F   # LATIN SMALL LETTER DOTLESS J WITH STROKE
<Multi_key> <0> <minus>             : "Œ∏"  U03B8   # GREEK SMALL LETTER THETA
<Multi_key> <o> <bar>               : "…µ"  U0275   # LATIN SMALL LETTER BARRED O
<Multi_key> <O> <bar>               : "Œ∏"  U03B8   # GREEK SMALL LETTER THETA
<Multi_key> <b> <apostrophe>        : "…ì"  U0253   # LATIN SMALL LETTER B WITH HOOK
<Multi_key> <h> <apostrophe>        : "…¶"  U0266   # LATIN SMALL LETTER H WITH HOOK
<Multi_key> <v> <apostrophe>        : "‚±±"  U2C71   # LATIN SMALL LETTER V WITH RIGHT HOOK

<Multi_key> <asciicircum> <asciicircum>  : " å"  U028C   # LATIN SMALL LETTER TURNED V
<Multi_key> <question> <period>     : " î"  U0294   # LATIN LETTER GLOTTAL STOP
<Multi_key> <question> <minus>      : " °"  U02A1   # LATIN LETTER GLOTTAL STOP WITH STROKE
<Multi_key> <e> <3>                 : "…ú"  U025C   # LATIN SMALL LETTER REVERSED OPEN E
<Multi_key> <o> <g>                 : "…§"  U0264   # LATIN SMALL LETTER RAMS HORN
<Multi_key> <o> <parenright>        : "…î"  U0254   # LATIN SMALL LETTER OPEN O
<Multi_key> <v> <backslash>         : " ã"  U028B   # LATIN SMALL LETTER V WITH HOOK

<Multi_key> <l> <z>                 : "…Æ"  U026E   # LATIN SMALL LETTER LEZH
<Multi_key> <o> <E>                 : "…∂"  U0276   # LATIN LETTER SMALL CAPITAL OE

<Multi_key> <g> <g>                 : "À†"  U02E0   # MODIFIER LETTER SMALL GAMMA
<Multi_key> <h> <h>                 : " ∞"  U02B0   # MODIFIER LETTER SMALL H
<Multi_key> <j> <j>                 : " ≤"  U02B2   # MODIFIER LETTER SMALL J
<Multi_key> <l> <l>                 : "À°"  U02E1   # MODIFIER LETTER SMALL L
<Multi_key> <n> <n>                 : "‚Åø"  U207F   # SUPERSCRIPT LATIN SMALL LETTER N
<Multi_key> <w> <w>                 : " ∑"  U02B7   # MODIFIER LETTER SMALL W
