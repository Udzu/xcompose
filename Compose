# -*- coding: utf-8; mode: conf -*-

# Some additional Compose key sequences, focusing on maths, linguistics and general text input.
# To use them, copy all or parts of this config into your ~/.XCompose file.
#
# For consistency and simplicity, a few of the rules here conflict with the system files, with
# workarounds given where possible in the comments (e.g. <underscore>'s usefulness for subscripting
# often interferes with its system use for macrons, for which <minus> works as an alternative).
#
# For a simple command line tool to help manage your compose configuration, check out
# https://github.com/Udzu/xcompose

include "%L"

###############################
# A. COMMON SCRIPT CHARACTERS #
###############################

##########
# Spaces #
##########

<Multi_key> <space> <space>      : " "  U00A0   # NO-BREAK SPACE
<Multi_key> <space> <n>          : " "  U2002   # EN SPACE
<Multi_key> <space> <m>          : " "  U2003   # EM SPACE
<Multi_key> <space> <3>          : " "  U2004   # THREE-PER-EM SPACE
<Multi_key> <space> <4>          : " "  U2005   # FOUR-PER-EM SPACE
<Multi_key> <space> <6>          : " "  U2006   # SIX-PER-EM SPACE
<Multi_key> <space> <numbersign> : " "  U2007   # FIGURE SPACE
<Multi_key> <space> <period>     : " "  U2008   # PUNCTUATION SPACE
<Multi_key> <space> <t>          : " "  U2009   # THIN SPACE
<Multi_key> <space> <h>          : " "  U200A   # HAIR SPACE
<Multi_key> <space> <bar>        : " "  U200A   # HAIR SPACE
<Multi_key> <space> <0>          : "​"  U200B   # ZERO WIDTH SPACE
<Multi_key> <space> <plus>       : " "  U205F   # MEDIUM MATHEMATICAL SPACE
<Multi_key> <backslash> <space>  : "␣"  U2423   # OPEN BOX

##########
# Dashes #
##########

<Multi_key> <minus> <space>          : "­"  U00AD   # SOFT HYPHEN (conflicts with useless ~)
<Multi_key> <minus> <Return>         : "‑"  U2011   # NON-BREAKING HYPHEN
<Multi_key> <minus> <numbersign>     : "‒"  U2012   # FIGURE DASH
<Multi_key> <minus> <n>              : "–"  U2013   # EN DASH
<Multi_key> <minus> <m>              : "—"  U2014   # EM DASH
<Multi_key> <minus> <quotedbl>       : "―"  U2015   # HORIZONTAL BAR (QUOTATION DASH)
<Multi_key> <minus> <asciitilde>     : "⁓"  U2053   # SWUNG DASH
<Multi_key> <minus> <2>              : "⸺"  U2E3A   # TWO-EM DASH
<Multi_key> <minus> <3>              : "⸻"  U2E3B   # THREE-EM DASH

############
# Brackets #
############

<Multi_key> <bar> <bar>                   : "‖"  U2016   # DOUBLE VERTICAL LINE
<Multi_key> <bracketleft> <minus>         : "⁅"  U2045   # LEFT SQUARE BRACKET WITH QUILL
<Multi_key> <bracketright> <minus>        : "⁆"  U2046   # RIGHT SQUARE BRACKET WITH QUILL
<Multi_key> <bracketleft> <asciicircum>   : "⌈"  U2308   # LEFT CEILING
<Multi_key> <bracketright> <asciicircum>  : "⌉"  U2309   # RIGHT CEILING
<Multi_key> <bracketleft> <underscore>    : "⌊"  U230A   # LEFT FLOOR
<Multi_key> <bracketright> <underscore>   : "⌋"  U230B   # RIGHT FLOOR
<Multi_key> <bracketleft> <bracketleft>   : "⟦"  U27E6   # MATHEMATICAL LEFT WHITE SQUARE BRACKET
<Multi_key> <bracketright> <bracketright> : "⟧"  U27E7   # MATHEMATICAL RIGHT WHITE SQUARE BRACKET
<Multi_key> <less> <parenleft>            : "⟨"  U27E8   # MATHEMATICAL LEFT ANGLE BRACKET
<Multi_key> <greater> <parenright>        : "⟩"  U27E9   # MATHEMATICAL RIGHT ANGLE BRACKET
<Multi_key> <parenleft> <less> <less>     : "⟪"  U27EA   # MATHEMATICAL LEFT DOUBLE ANGLE BRACKET
<Multi_key> <parenright> <greater> <greater> : "⟫"  U27EB   # MATHEMATICAL RIGHT DOUBLE ANGLE BRACKET
<Multi_key> <parenleft> <slash> <slash>   : "⫽"  U2AFD   # DOUBLE SOLIDUS OPERATOR
<Multi_key> <parenright> <slash> <slash>  : "⫽"  U2AFD   # DOUBLE SOLIDUS OPERATOR
<Multi_key> <slash> <slash> <slash>       : "⫽"  U2AFD   # DOUBLE SOLIDUS OPERATOR (conflicts with useless \)
<Multi_key> <parenleft> <parenleft>       : "⸨"  U2E28   # LEFT DOUBLE PARENTHESIS (conflicts with useless [)
<Multi_key> <parenright> <parenright>     : "⸩"  U2E29   # RIGHT DOUBLE PARENTHESIS (conflicts with useless ])

###########
# Bullets #
###########

<Multi_key> <asterisk> <asterisk>   : "•" U2022    # BULLET
<Multi_key> <asterisk> <greater>    : "‣" U2023    # TRIANGULAR BULLET
<Multi_key> <asterisk> <minus>      : "⁃" U2043    # HYPHEN BULLET
<Multi_key> <asterisk> <parenleft>  : "⁌" U204C    # BLACK LEFTWARDS BULLET
<Multi_key> <asterisk> <parenright> : "⁍" U204D    # BLACK RIGHTWARDS BULLET
<Multi_key> <asterisk> <at>         : "◉" U25C9    # FISHEYE (TAINOME)
<Multi_key> <asterisk> <o>          : "◦" U25E6    # WHITE BULLET

########################
# Combining diacritics #
########################

# Convention: all combining character sequences end with <Return>

<Multi_key> <grave> <Return>         : "̀"  U0300   # COMBINING GRAVE ACCENT
<Multi_key> <apostrophe> <Return>    : "́"  U0301   # COMBINING ACUTE ACCENT
<Multi_key> <asciicircum> <Return>   : "̂"  U0302   # COMBINING CIRCUMFLEX ACCENT
<Multi_key> <asciitilde> <Return>    : "̃"  U0303   # COMBINING TILDE
<Multi_key> <underscore> <Return>    : "̄"  U0304   # COMBINING MACRON
<Multi_key> <asciicircum> <underscore> <Return> : "̅"  U0305   # COMBINING OVERLINE
<Multi_key> <u> <Return>             : "̆"  U0306   # COMBINING BREVE
<Multi_key> <period> <Return>        : "̇"  U0307   # COMBINING DOT ABOVE
<Multi_key> <quotedbl> <Return>      : "̈"  U0308   # COMBINING DIAERESIS
<Multi_key> <question> <Return>      : "̉"  U0309   # COMBINING HOOK ABOVE
<Multi_key> <o> <Return>             : "̊"  U030A   # COMBINING RING ABOVE
<Multi_key> <equal> <Return>         : "̋"  U030B   # COMBINING DOUBLE ACUTE ACCENT
<Multi_key> <v> <Return>             : "̌"  U030C   # COMBINING CARON
<Multi_key> <bar> <Return>           : "̍"  U030D   # COMBINING VERTICAL LINE ABOVE
<Multi_key> <grave> <grave> <Return> : "̏"  U030F   # COMBINING DOUBLE GRAVE ACCENT
<Multi_key> <u> <v> <Return>         : "̑"  U0311   # COMBINING INVERTED BREVE
<Multi_key> <asciicircum> <comma> <Return> : "̓"  U0313   # COMBINING COMMA ABOVE (less important than cedilla)
<Multi_key> <comma> <less> <Return>  : "̔"  U0314   # COMBINING REVERSED COMMA ABOVE
<Multi_key> <notsign> <Return>       : "̚"  U031A   # COMBINING LEFT ANGLE ABOVE
<Multi_key> <Multi_key> <minus> <comma> <Return> : "̚"  U031A   # COMBINING LEFT ANGLE ABOVE
<Multi_key> <exclam> <Return>        : "̣"  U0323   # COMBINING DOT BELOW
<Multi_key> <comma> <Return>         : "̧"  U0327   # COMBINING CEDILLA
<Multi_key> <semicolon> <Return>     : "̨"  U0328   # COMBINING OGONEK
<Multi_key> <bracketleft> <Return>   : "͆"  U0346   # COMBINING BRIDGE ABOVE
<Multi_key> <asciitilde> <slash> <Return> : "͊"  U034A   # COMBINING NOT TILDE ABOVE
<Multi_key> <slash> <asciitilde> <Return> : "͊"  U034A   # COMBINING NOT TILDE ABOVE
<Multi_key> <parenright> <Return>    : "͝"  U035D   # COMBINING DOUBLE BREVE  
<Multi_key> <parenleft> <Return>     : "͡"  U0361   # COMBINING DOUBLE INVERTED BREVE

<Multi_key> <underscore> <grave> <Return>            : "̖"  U0316   # COMBINING GRAVE ACCENT BELOW
<Multi_key> <underscore> <apostrophe> <Return>       : "̗"  U0317   # COMBINING ACUTE ACCENT BELOW (overrides APL ⍘)
<Multi_key> <underscore> <comma> <less> <Return>     : "̜"  U031C   # COMBINING LEFT HALF RING BELOW
<Multi_key> <underscore> <bar> <underscore> <Return> : "̝"  U031D   # COMBINING UP TACK BELOW
<Multi_key> <underscore> <T> <Return>                : "̞"  U031E   # COMBINING DOWN TACK BELOW
<Multi_key> <underscore> <underscore> <plus> <Return> : "̟"  U031F   # COMBINING PLUS SIGN BELOW (conflicts with ū, use -u)
<Multi_key> <underscore> <underscore> <minus> <Return> : "̠"  U0320   # COMBINING MINUS SIGN BELOW (conflicts with ū, use -u)
<Multi_key> <underscore> <underscore> <j> <Return>   : "̡"  U0321   # COMBINING PALATALIZED HOOK BELOW (conflicts with ū, use -u)
<Multi_key> <underscore> <underscore> <l> <Return>   : "̢"  U0322   # COMBINING RETROFLEX HOOK BELOW (conflicts with ū, use -u)
<Multi_key> <underscore> <period> <Return>           : "̣"  U0323   # COMBINING DOT BELOW
<Multi_key> <underscore> <quotedbl> <Return>         : "̤"  U0324   # COMBINING DIAERESIS BELOW
<Multi_key> <underscore> <o> <Return>                : "̥"  U0325   # COMBINING RING BELOW (conflicts with ō, use -o)
<Multi_key> <underscore> <underscore> <comma> <Return> : "̦"  U0326   # COMBINING COMMA BELOW (conflicts with ū, use -u)
<Multi_key> <underscore> <comma> <Return>            : "̧"  U0327   # COMBINING CEDILLA
<Multi_key> <underscore> <semicolon> <Return>        : "̨"  U0328   # COMBINING OGONEK
<Multi_key> <underscore> <bar> <Return>              : "̩"  U0329   # COMBINING VERTICAL LINE BELOW
<Multi_key> <underscore> <bracketleft> <Return>      : "̪"  U032A   # COMBINING BRIDGE BELOW
<Multi_key> <underscore> <w> <Return>                : "̫"  U032B   # COMBINING INVERTED DOUBLE ARCH BELOW
<Multi_key> <underscore> <v> <Return>                : "̬"  U032C   # COMBINING CARON BELOW
<Multi_key> <underscore> <asciicircum> <Return>      : "̭"  U032D   # COMBINING CIRCUMFLEX ACCENT BELOW (conflicts with ¯, use -^)
<Multi_key> <underscore> <u> <Return>                : "̮"  U032E   # COMBINING BREVE BELOW (conflicts with ū, use -u)
<Multi_key> <underscore> <u> <v> <Return>            : "̯"  U032F   # COMBINING INVERTED BREVE BELOW (conflicts with ū, use -u)
<Multi_key> <underscore> <asciitilde> <Return>       : "̰"  U0330   # COMBINING TILDE BELOW
<Multi_key> <underscore> <underscore> <Return>       : "̱"  U0331   # COMBINING MACRON BELOW (conflicts with ¯, use -^)
<Multi_key> <underscore> <underscore> <underscore> <Return> : "̲"  U0332   # COMBINING LOW LINE (UNDERLINE) (conflicts with ¯, use -^)
<Multi_key> <underscore> <underscore> <equal> <Return> : "̳"  U0333   # COMBINING DOUBLE LOW LINE (UNDERLINE) (conflicts with ¯, use -^)
<Multi_key> <underscore> <bracketright> <Return>     : "̺"  U033A   # COMBINING INVERTED BRIDGE BELOW
<Multi_key> <underscore> <bracketleft> <bracketright> <Return> : "̻"  U033B   # COMBINING SQUARE BELOW
<Multi_key> <underscore> <M> <Return>                : "̼"  U033C   # COMBINING SEAGULL BELOW
<Multi_key> <underscore> <underscore> <underscore> <equal> <Return> : "͇"  U0347   # COMBINING EQUALS SIGN BELOW (conflicts with ū, use -u)
<Multi_key> <underscore> <bar> <bar> <Return>        : "͈"  U0348   # COMBINING DOUBLE VERTICAL LINE BELOW
<Multi_key> <underscore> <notsign> <Return>          : "͉"  U0349   # COMBINING LEFT ANGLE BELOW
<Multi_key> <underscore> <Multi_key> <minus> <comma> <Return> : "͉"  U0349   # COMBINING LEFT ANGLE BELOW
<Multi_key> <underscore> <underscore> <parenright> <Return> : "͜"  U035C   # COMBINING DOUBLE BREVE BELOW (conflicts with ¯, use -^)

<Multi_key> <minus> <minus> <Return>         : "̶"  U0336   # COMBINING LONG STROKE OVERLAY (STRIKETHROUGH)
<Multi_key> <minus> <slash> <Return>         : "̸"  U0338   # COMBINING LONG SOLIDUS OVERLAY (overrides APL ⌿)
<Multi_key> <minus> <bar> <Return>           : "⃒"  U20D2   # COMBINING LONG VERTICAL LINE OVERLAY
<Multi_key> <minus> <backslash> <Return>     : "⃥"  U20E5   # COMBINING REVERSE SOLIDUS OVERLAY (overrides APL ⍀)
<Multi_key> <minus> <bar> <bar> <Return>     : "⃦"  U20E6   # COMBINING DOUBLE VERTICAL STROKE OVERLAY
<Multi_key> <minus> <less> <minus> <Return>  : "⃪"  U20EA   # COMBINING LEFTWARDS ARROW OVERLAY
<Multi_key> <minus> <slash> <slash> <Return> : "⃫"  U20EB   # COMBINING LONG DOUBLE SOLIDUS OVERLAY (overrides APL ⌿)

####################################
# Bidirectional control characters #
####################################

<Multi_key> <l> <r> <m> : "‎"  U200E   # LEFT-TO-RIGHT MARK
<Multi_key> <r> <l> <m> : "‏"  U200F   # RIGHT-TO-LEFT MARK
<Multi_key> <l> <r> <e> : "‪"  U202A   # LEFT-TO-RIGHT EMBEDDING
<Multi_key> <r> <l> <e> : "‫"  U202B   # RIGHT-TO-LEFT EMBEDDING
<Multi_key> <p> <d> <f> : "‬"  U202C   # POP DIRECTIONAL FORMATTING
<Multi_key> <l> <r> <o> : "‭"  U202D   # LEFT-TO-RIGHT OVERRIDE
<Multi_key> <r> <l> <o> : "‮"  U202E   # RIGHT-TO-LEFT OVERRIDE
<Multi_key> <l> <r> <i> : "⁦"  U2066   # LEFT-TO-RIGHT ISOLATE
<Multi_key> <r> <l> <i> : "⁧"  U2067   # RIGHT-TO-LEFT ISOLATE
<Multi_key> <p> <d> <i> : "⁩"  U2069   # POP DIRECTIONAL ISOLATE


##############################
# B. LATIN SCRIPT CHARACTERS #
##############################

###########################################################
# International Phonetic Alphabet (and other linguistics) #
###########################################################

# Also see brackets and combining diacritics sections

<Multi_key> <b> <h>                 : "β"  U03B2   # GREEK SMALL LETTER BETA
<Multi_key> <g> <h>                 : "ɣ"  U0263   # LATIN SMALL LETTER GAMMA
<Multi_key> <j> <h>                 : "ʝ"  U029D   # LATIN SMALL LETTER J WITH CROSSED-TAIL
<Multi_key> <l> <h>                 : "ɬ"  U026C   # LATIN SMALL LETTER L WITH BELT
<Multi_key> <p> <h>                 : "ɸ"  U0278   # LATIN SMALL LETTER PHI
<Multi_key> <q> <h>                 : "χ"  U03C7   # GREEK SMALL LETTER CHI (alternatively xh)
<Multi_key> <s> <h>                 : "ʃ"  U0283   # LATIN SMALL LETTER ESH
<Multi_key> <x> <h>                 : "χ"  U03C7   # GREEK SMALL LETTER CHI
<Multi_key> <z> <h>                 : "ʒ"  U0292   # LATIN SMALL LETTER EZH
<Multi_key> <g> <n>                 : "ɲ"  U0272   # LATIN SMALL LETTER N WITH LEFT HOOK
<Multi_key> <m> <f>                 : "ɱ"  U0271   # LATIN SMALL LETTER M WITH HOOK
<Multi_key> <m> <g>                 : "ɱ"  U0271   # LATIN SMALL LETTER M WITH HOOK
<Multi_key> <n> <g>                 : "ŋ"  U014B   # LATIN SMALL LETTER ENG
<Multi_key> <r> <d>                 : "ɾ"  U027E   # LATIN SMALL LETTER R WITH FISHHOOK
<Multi_key> <g> <w>                 : "ɰ"  U0270   # LATIN SMALL LETTER TURNED M WITH LONG LEG
<Multi_key> <v> <w>                 : "ʋ"  U028B   # LATIN SMALL LETTER V WITH HOOK

<Multi_key> <a> <h>                 : "ɑ"  U0251   # LATIN SMALL LETTER ALPHA
<Multi_key> <e> <h>                 : "ɛ"  U025B   # LATIN SMALL LETTER OPEN E
<Multi_key> <i> <h>                 : "ɪ"  U026A   # LATIN LETTER SMALL CAPITAL I
<Multi_key> <u> <h>                 : "ʊ"  U028A   # LATIN SMALL LETTER UPSILON
<Multi_key> <w> <h>                 : "ɯ"  U026F   # LATIN SMALL LETTER TURNED M
<Multi_key> <y> <h>                 : "ʏ"  U028F   # LATIN LETTER SMALL CAPITAL Y
<Multi_key> <e> <r>                 : "ɚ"  U025A   # LATIN SMALL LETTER SCHWA WITH HOOK

<Multi_key> <b> <B>                 : "ʙ"  U0299   # LATIN LETTER SMALL CAPITAL B
<Multi_key> <B> <B>                 : "ʙ"  U0299   # LATIN LETTER SMALL CAPITAL B
<Multi_key> <G> <G>                 : "ɢ"  U0262   # LATIN LETTER SMALL CAPITAL G
<Multi_key> <i> <I>                 : "ɪ"  U026A   # LATIN LETTER SMALL CAPITAL I
<Multi_key> <I> <I>                 : "ɪ"  U026A   # LATIN LETTER SMALL CAPITAL I
<Multi_key> <h> <H>                 : "ʜ"  U029C   # LATIN LETTER SMALL CAPITAL H
<Multi_key> <H> <H>                 : "ʜ"  U029C   # LATIN LETTER SMALL CAPITAL H
<Multi_key> <l> <L>                 : "ʟ"  U029F   # LATIN LETTER SMALL CAPITAL L
<Multi_key> <n> <N>                 : "ɴ"  U0274   # LATIN LETTER SMALL CAPITAL N
<Multi_key> <r> <R>                 : "ʀ"  U0280   # LATIN LETTER SMALL CAPITAL R
<Multi_key> <y> <Y>                 : "ʏ"  U028F   # LATIN LETTER SMALL CAPITAL Y
<Multi_key> <Y> <Y>                 : "ʏ"  U028F   # LATIN LETTER SMALL CAPITAL Y

<Multi_key> <e> <e>                 : "ə"  U0259   # LATIN SMALL LETTER SCHWA
<Multi_key> <r> <r>                 : "ɹ"  U0279   # LATIN SMALL LETTER TURNED R

<Multi_key> <a> <v>                 : "ɐ"  U0250   # LATIN SMALL LETTER TURNED A
<Multi_key> <e> <v>                 : "ə"  U0259   # LATIN SMALL LETTER SCHWA
<Multi_key> <h> <v>                 : "ɥ"  U0265   # LATIN SMALL LETTER TURNED H
<Multi_key> <m> <v>                 : "ɯ"  U026F   # LATIN SMALL LETTER TURNED M
<Multi_key> <r> <v>                 : "ɹ"  U0279   # LATIN SMALL LETTER TURNED R
<Multi_key> <R> <v>                 : "ʁ"  U0281   # LATIN LETTER SMALL CAPITAL INVERTED R
<Multi_key> <v> <v>                 : "ʌ"  U028C   # LATIN SMALL LETTER TURNED V
<Multi_key> <W> <v>                 : "ʍ"  U028D   # LATIN SMALL LETTER TURNED W
<Multi_key> <y> <v>                 : "ʎ"  U028E   # LATIN SMALL LETTER TURNED Y

<Multi_key> <a> <less>              : "ɒ"  U0252   # LATIN SMALL LETTER TURNED ALPHA (close enough)
<Multi_key> <e> <less>              : "ɘ"  U0258   # LATIN SMALL LETTER REVERSED E (conflicts with ě, use ve)
<Multi_key> <question> <less> <period> : "ʕ"  U0295   # LATIN LETTER PHARYNGEAL VOICED FRICATIVE
<Multi_key> <question> <less> <minus> : "ʢ"  U02A2   # LATIN LETTER REVERSED GLOTTAL STOP WITH STROKE

<Multi_key> <d> <l>                 : "ɖ"  U0256   # LATIN SMALL LETTER D WITH TAIL
<Multi_key> <n> <l>                 : "ɳ"  U0273   # LATIN SMALL LETTER N WITH RETROFLEX HOOK
<Multi_key> <s> <l>                 : "ʂ"  U0282   # LATIN SMALL LETTER S WITH HOOK
<Multi_key> <z> <l>                 : "ʐ"  U0290   # LATIN SMALL LETTER Z WITH RETROFLEX HOOK
<Multi_key> <r> <l> <Return>        : "ɽ"  U027D   # LATIN SMALL LETTER R WITH TAIL
<Multi_key> <r> <l> <v>             : "ɻ"  U027B   # LATIN SMALL LETTER TURNED R WITH HOOK
<Multi_key> <c> <at>                : "ɕ"  U0255   # LATIN SMALL LETTER C WITH CURL
<Multi_key> <z> <at>                : "ʑ"  U0291   # LATIN SMALL LETTER Z WITH CURL
<Multi_key> <h> <minus>             : "ħ"  U0127   # LATIN SMALL LETTER H WITH STROKE
<Multi_key> <j> <minus>             : "ɟ"  U025F   # LATIN SMALL LETTER DOTLESS J WITH STROKE
<Multi_key> <0> <minus>             : "θ"  U03B8   # GREEK SMALL LETTER THETA
<Multi_key> <o> <bar>               : "ɵ"  U0275   # LATIN SMALL LETTER BARRED O
<Multi_key> <O> <bar>               : "θ"  U03B8   # GREEK SMALL LETTER THETA
<Multi_key> <b> <apostrophe>        : "ɓ"  U0253   # LATIN SMALL LETTER B WITH HOOK
<Multi_key> <h> <apostrophe>        : "ɦ"  U0266   # LATIN SMALL LETTER H WITH HOOK
<Multi_key> <v> <apostrophe>        : "ⱱ"  U2C71   # LATIN SMALL LETTER V WITH RIGHT HOOK

<Multi_key> <asciicircum> <asciicircum>  : "ʌ"  U028C   # LATIN SMALL LETTER TURNED V
<Multi_key> <question> <period>     : "ʔ"  U0294   # LATIN LETTER GLOTTAL STOP
<Multi_key> <question> <minus>      : "ʡ"  U02A1   # LATIN LETTER GLOTTAL STOP WITH STROKE
<Multi_key> <e> <3>                 : "ɜ"  U025C   # LATIN SMALL LETTER REVERSED OPEN E
<Multi_key> <o> <g>                 : "ɤ"  U0264   # LATIN SMALL LETTER RAMS HORN
<Multi_key> <o> <parenright>        : "ɔ"  U0254   # LATIN SMALL LETTER OPEN O
<Multi_key> <v> <backslash>         : "ʋ"  U028B   # LATIN SMALL LETTER V WITH HOOK

<Multi_key> <l> <z>                 : "ɮ"  U026E   # LATIN SMALL LETTER LEZH
<Multi_key> <o> <E>                 : "ɶ"  U0276   # LATIN LETTER SMALL CAPITAL OE

<Multi_key> <g> <g>                 : "ˠ"  U02E0   # MODIFIER LETTER SMALL GAMMA
<Multi_key> <h> <h>                 : "ʰ"  U02B0   # MODIFIER LETTER SMALL H
<Multi_key> <j> <j>                 : "ʲ"  U02B2   # MODIFIER LETTER SMALL J
<Multi_key> <l> <l>                 : "ˡ"  U02E1   # MODIFIER LETTER SMALL L
<Multi_key> <n> <n>                 : "ⁿ"  U207F   # SUPERSCRIPT LATIN SMALL LETTER N
<Multi_key> <w> <w>                 : "ʷ"  U02B7   # MODIFIER LETTER SMALL W
<Multi_key> <T> <T>                 : "˕"  U02D5   # MODIFIER LETTER DOWN TACK
<Multi_key> <T> <v> <T>             : "˔"  U02D4   # MODIFIER LETTER UP TACK
<Multi_key> <0> <0> <minus>         : "ᶿ"  U1DBF   # MODIFIER LETTER SMALL THETA
<Multi_key> <O> <O> <bar>           : "ᶿ"  U1DBF   # MODIFIER LETTER SMALL THETA
<Multi_key> <x> <asciicircum> <x>   : "ˣ"  U02E3   # MODIFIER LETTER SMALL X
