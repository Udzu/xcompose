# bars, codas, repetitions and pauses
<Multi_key> <numbersign> <bar> <period>      : "𝄀" U1D100    # MUSICAL SYMBOL SINGLE BARLINE
<Multi_key> <numbersign> <bar> <bar> <period> : "𝄁" U1D101    # MUSICAL SYMBOL DOUBLE BARLINE
<Multi_key> <numbersign> <bar> <bar> <bar>   : "𝄂" U1D102    # MUSICAL SYMBOL FINAL BARLINE
<Multi_key> <numbersign> <period> <bar>      : "𝄄" U1D104    # MUSICAL SYMBOL DASHED BARLINE
<Multi_key> <numbersign> <bar> <bar> <colon> : "𝄆" U1D106    # MUSICAL SYMBOL LEFT REPEAT SIGN
<Multi_key> <numbersign> <colon> <bar> <bar> : "𝄇" U1D107    # MUSICAL SYMBOL RIGHT REPEAT SIGN
<Multi_key> <numbersign> <d> <s>             : "𝄉" U1D109    # MUSICAL SYMBOL DAL SEGNO
<Multi_key> <numbersign> <d> <c>             : "𝄊" U1D10A    # MUSICAL SYMBOL DA CAPO
