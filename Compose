<Multi_key> <m> <x> <apostrophe> <e>  : "··-··"   # MORSE CODE É
<Multi_key> <m> <x> <comma> <e>       : "··-··"   # MORSE CODE Ę
<Multi_key> <m> <x> <d> <h>           : "··--·"   # MORSE CODE Ð (ð)
<Multi_key> <m> <x> <grave> <e>       : "·-··-"   # MORSE CODE È
<Multi_key> <m> <x> <slash> <l>       : "·-··-"   # MORSE CODE Ł
<Multi_key> <m> <x> <asciicircum> <g> : "--·-·"   # MORSE CODE Ĝ
<Multi_key> <m> <x> <asciicircum> <j> : "·---·"   # MORSE CODE Ĵ
<Multi_key> <m> <x> <apostrophe> <n>  : "--·--"   # MORSE CODE Ń
<Multi_key> <m> <x> <asciitilde> <n>  : "--·--"   # MORSE CODE Ñ
<Multi_key> <m> <x> <apostrophe> <o>  : "---·"    # MORSE CODE Ó
<Multi_key> <m> <x> <quotedbl> <o>    : "---·"    # MORSE CODE Ö
<Multi_key> <m> <x> <slash> <o>       : "---·"    # MORSE CODE Ø
<Multi_key> <m> <x> <apostrophe> <s>  : "···-···" # MORSE CODE Ś
<Multi_key> <m> <x> <asciicircum> <s> : "···-·"   # MORSE CODE Ŝ
<Multi_key> <m> <x> <t> <h>           : "·--··"   # MORSE CODE Þ
<Multi_key> <m> <x> <quotedbl> <u>    : "··--"    # MORSE CODE Ü
<Multi_key> <m> <x> <v> <u>           : "··--"    # MORSE CODE Ǔ
<Multi_key> <m> <x> <apostrophe> <z>  : "--··-·"  # MORSE CODE Ź
<Multi_key> <m> <x> <period> <z>      : "--··-"   # MORSE CODE Ż

<Multi_key> <m> <numbersign> <0> : "-----"  # MORSE CODE 0
<Multi_key> <m> <numbersign> <1> : "·----"  # MORSE CODE 1
<Multi_key> <m> <numbersign> <2> : "··---"  # MORSE CODE 2
<Multi_key> <m> <numbersign> <3> : "···--"  # MORSE CODE 3
<Multi_key> <m> <numbersign> <4> : "····-"  # MORSE CODE 4
<Multi_key> <m> <numbersign> <5> : "·····"  # MORSE CODE 5
<Multi_key> <m> <numbersign> <6> : "-····"  # MORSE CODE 6
<Multi_key> <m> <numbersign> <7> : "--···"  # MORSE CODE 7
<Multi_key> <m> <numbersign> <8> : "---··"  # MORSE CODE 8
<Multi_key> <m> <numbersign> <9> : "----·"  # MORSE CODE 9

<Multi_key> <m> <semicolon> <apostrophe> : "·----·"  # MORSE CODE '
<Multi_key> <m> <semicolon> <at>         : "·--·-·"  # MORSE CODE @
<Multi_key> <m> <semicolon> <parenleft>  : "-·--·"   # MORSE CODE (
<Multi_key> <m> <semicolon> <parenright> : "-·--·-"  # MORSE CODE )
<Multi_key> <m> <semicolon> <colon>      : "---···"  # MORSE CODE :
<Multi_key> <m> <semicolon> <comma>      : "--··--"  # MORSE CODE ,
<Multi_key> <m> <semicolon> <equal>      : "-···-"   # MORSE CODE =
<Multi_key> <m> <semicolon> <period>     : "·-·-·-"  # MORSE CODE .
<Multi_key> <m> <semicolon> <minus>      : "-····-"  # MORSE CODE -
<Multi_key> <m> <semicolon> <plus>       : "·-·-·"   # MORSE CODE +
<Multi_key> <m> <semicolon> <quotedbl>   : "·-··-·"  # MORSE CODE "
<Multi_key> <m> <semicolon> <question>   : "··--··"  # MORSE CODE ?
<Multi_key> <m> <semicolon> <slash>      : "-··-·"   # MORSE CODE /
<Multi_key> <m> <semicolon> <ampersand>  : "·-···"   # MORSE CODE & (nonstandard)
<Multi_key> <m> <semicolon> <exclam>     : "-·-·--"  # MORSE CODE ! (nonstandard)
<Multi_key> <m> <semicolon> <dollar>     : "···-··-" # MORSE CODE $ (nonstandard)
<Multi_key> <m> <semicolon> <semicolon>  : "-·-·-·"  # MORSE CODE ; (nonstandard)
<Multi_key> <m> <semicolon> <underscore> : "··--·-"  # MORSE CODE _ (nonstandard)

# (p = prosign)

<Multi_key> <m> <p> <a> <a>     : "·-·-"   # MORSE CODE PROSIGN AA (UNKNOWN STATION)
<Multi_key> <m> <p> <a> <r>     : "·-·-·"  # MORSE CODE PROSIGN AR (OUT)
