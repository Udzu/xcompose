<Multi_key> <numbersign> <at> <c>            : "𝄡" U1D121    # MUSICAL SYMBOL C CLEF
<Multi_key> <numbersign> <at> <f>            : "𝄢" U1D122    # MUSICAL SYMBOL F CLEF
<Multi_key> <numbersign> <at> <asciicircum> <8> <f> : "𝄣" U1D123    # MUSICAL SYMBOL F CLEF OTTAVA ALTA
<Multi_key> <numbersign> <at> <underscore> <8> <f>  : "𝄤" U1D124    # MUSICAL SYMBOL F CLEF OTTAVA BASSA
<Multi_key> <numbersign> <at> <n> <1>        : "𝄥" U1D125    # MUSICAL SYMBOL DRUM CLEF-1 (NEUTRAL CLEF)
<Multi_key> <numbersign> <at> <n> <2>        : "𝄦" U1D126    # MUSICAL SYMBOL DRUM CLEF-2 (NEUTRAL CLEF)
<Multi_key> <numbersign> <at> <d> <1>        : "𝄥" U1D125    # MUSICAL SYMBOL DRUM CLEF-1
