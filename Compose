# -*- coding: utf-8; mode: conf -*-
include "%L"

# Some additional Compose key sequences, focusing on maths, linguistics and general text input.
# To use them, copy all or parts of this config into your ~/.XCompose file.
#
# For consistency and simplicity, a few of the rules here conflict with the system files, with
# workarounds given where possible in the comments (e.g. <underscore>'s usefulness for subscripting
# often interferes with its system use for macrons, for which <minus> works as an alternative).
#
# For a simple command line tool to help manage your compose configuration, check out
# https://github.com/Udzu/xcompose

##########
# Spaces #
##########

<Multi_key> <space> <space>      : "¬†"  U00A0   # NO-BREAK SPACE
<Multi_key> <space> <n>          : "‚ÄÇ"  U2002   # EN SPACE
<Multi_key> <space> <m>          : "‚ÄÉ"  U2003   # EM SPACE
<Multi_key> <space> <3>          : "‚ÄÑ"  U2004   # THREE-PER-EM SPACE
<Multi_key> <space> <4>          : "‚ÄÖ"  U2005   # FOUR-PER-EM SPACE
<Multi_key> <space> <6>          : "‚ÄÜ"  U2006   # SIX-PER-EM SPACE
<Multi_key> <space> <numbersign> : "‚Äá"  U2007   # FIGURE SPACE
<Multi_key> <space> <period>     : "‚Äà"  U2008   # PUNCTUATION SPACE
<Multi_key> <space> <t>          : "‚Äâ"  U2009   # THIN SPACE
<Multi_key> <space> <h>          : "‚Ää"  U200A   # HAIR SPACE
<Multi_key> <space> <bar>        : "‚Ää"  U200A   # HAIR SPACE
<Multi_key> <space> <0>          : "‚Äã"  U200B   # ZERO WIDTH SPACE
<Multi_key> <space> <plus>       : "‚Åü"  U205F   # MEDIUM MATHEMATICAL SPACE

##########
# Dashes #
##########

<Multi_key> <minus> <space>          : "¬≠"  U00AD   # SOFT HYPHEN (conflicts with useless ~)
<Multi_key> <minus> <Return>         : "‚Äë"  U2011   # NON-BREAKING HYPHEN
<Multi_key> <minus> <numbersign>     : "‚Äí"  U2012   # FIGURE DASH
<Multi_key> <minus> <n>              : "‚Äì"  U2013   # EN DASH
<Multi_key> <minus> <m>              : "‚Äî"  U2014   # EM DASH
<Multi_key> <minus> <asciitilde>     : "‚Åì"  U2053   # SWUNG DASH
<Multi_key> <minus> <2>              : "‚∏∫"  U2E3A   # TWO-EM DASH
<Multi_key> <minus> <3>              : "‚∏ª"  U2E3B   # THREE-EM DASH

############
# Brackets #
############

<Multi_key> <bar> <bar>                   : "‚Äñ"  U2016   # DOUBLE VERTICAL LINE
<Multi_key> <bracketleft> <bracketleft>   : "‚ü¶"  U27E6   # MATHEMATICAL LEFT WHITE SQUARE BRACKET
<Multi_key> <bracketright> <bracketright> : "‚üß"  U27E7   # MATHEMATICAL RIGHT WHITE SQUARE BRACKET
<Multi_key> <less> <parenleft>            : "‚ü®"  U27E8   # MATHEMATICAL LEFT ANGLE BRACKET
<Multi_key> <greater> <parenright>        : "‚ü©"  U27E9   # MATHEMATICAL RIGHT ANGLE BRACKET
<Multi_key> <slash> <slash> <slash>       : "‚´Ω"  U2AFD   # DOUBLE SOLIDUS OPERATOR (conflicts with useless \)
<Multi_key> <parenleft> <parenleft>       : "‚∏®"  U2E28   # LEFT DOUBLE PARENTHESIS (conflicts with useless [)
<Multi_key> <parenright> <parenright>     : "‚∏©"  U2E29   # RIGHT DOUBLE PARENTHESIS (conflicts with useless ])

########################
# Combining diacritics #
########################

# Convention: all combining character sequences end with <Return>

<Multi_key> <grave> <Return>         : "ÃÄ"  U0300   # COMBINING GRAVE ACCENT
<Multi_key> <apostrophe> <Return>    : "ÃÅ"  U0301   # COMBINING ACUTE ACCENT
<Multi_key> <asciicircum> <Return>   : "ÃÇ"  U0302   # COMBINING CIRCUMFLEX ACCENT
<Multi_key> <asciitilde> <Return>    : "ÃÉ"  U0303   # COMBINING TILDE
<Multi_key> <underscore> <Return>    : "ÃÑ"  U0304   # COMBINING MACRON
<Multi_key> <asciicircum> <underscore> <Return> : "ÃÖ"  U0305   # COMBINING OVERLINE
<Multi_key> <u> <Return>             : "ÃÜ"  U0306   # COMBINING BREVE
<Multi_key> <period> <Return>        : "Ãá"  U0307   # COMBINING DOT ABOVE
<Multi_key> <quotedbl> <Return>      : "Ãà"  U0308   # COMBINING DIAERESIS
<Multi_key> <question> <Return>      : "Ãâ"  U0309   # COMBINING HOOK ABOVE
<Multi_key> <o> <Return>             : "Ãä"  U030A   # COMBINING RING ABOVE
<Multi_key> <equal> <Return>         : "Ãã"  U030B   # COMBINING DOUBLE ACUTE ACCENT
<Multi_key> <v> <Return>             : "Ãå"  U030C   # COMBINING CARON
<Multi_key> <bar> <Return>           : "Ãç"  U030D   # COMBINING VERTICAL LINE ABOVE
<Multi_key> <grave> <grave> <Return> : "Ãè"  U030F   # COMBINING DOUBLE GRAVE ACCENT
<Multi_key> <u> <v> <Return>         : "Ãë"  U0311   # COMBINING INVERTED BREVE
<Multi_key> <asciicircum> <comma> <Return> : "Ãì"  U0313   # COMBINING COMMA ABOVE (less important than cedilla)
<Multi_key> <comma> <less> <Return>  : "Ãî"  U0314   # COMBINING REVERSED COMMA ABOVE
<Multi_key> <notsign> <Return>       : "Ãö"  U031A   # COMBINING LEFT ANGLE ABOVE
<Multi_key> <exclam> <Return>        : "Ã£"  U0323   # COMBINING DOT BELOW
<Multi_key> <comma> <Return>         : "Ãß"  U0327   # COMBINING CEDILLA
<Multi_key> <semicolon> <Return>     : "Ã®"  U0328   # COMBINING OGONEK
<Multi_key> <leftbracket> <Return>   : "ÕÜ"  U0346   # COMBINING BRIDGE ABOVE
<Multi_key> <asciitilde> <slash> <Return> : "Õä"  U034A   # COMBINING NOT TILDE ABOVE
<Multi_key> <slash> <asciitilde> <Return> : "Õä"  U034A   # COMBINING NOT TILDE ABOVE
<Multi_key> <parenright> <Return>    : "Õù"  U035D   # COMBINING DOUBLE BREVE  
<Multi_key> <parenleft> <Return>     : "Õ°"  U0361   # COMBINING DOUBLE INVERTED BREVE

<Multi_key> <underscore> <grave> <Return>            : "Ãñ"  U0316   # COMBINING GRAVE ACCENT BELOW
<Multi_key> <underscore> <apostrophe> <Return>       : "Ãó"  U0317   # COMBINING ACUTE ACCENT BELOW (overrides APL ‚çò)
<Multi_key> <underscore> <comma> <less> <Return>     : "Ãú"  U031C   # COMBINING LEFT HALF RING BELOW
<Multi_key> <underscore> <bar> <underscore> <Return> : "Ãù"  U031D   # COMBINING UP TACK BELOW
<Multi_key> <underscore> <T> <Return>                : "Ãû"  U031E   # COMBINING DOWN TACK BELOW
<Multi_key> <underscore> <underscore> <plus> <Return> : "Ãü"  U031F   # COMBINING PLUS SIGN BELOW (conflicts with ≈´, use -u)
<Multi_key> <underscore> <underscore> <minus> <Return> : "Ã†"  U0320   # COMBINING MINUS SIGN BELOW (conflicts with ≈´, use -u)
<Multi_key> <underscore> <underscore> <j> <Return>   : "Ã°"  U0321   # COMBINING PALATALIZED HOOK BELOW (conflicts with ≈´, use -u)
<Multi_key> <underscore> <underscore> <l> <Return>   : "Ã¢"  U0322   # COMBINING RETROFLEX HOOK BELOW (conflicts with ≈´, use -u)
<Multi_key> <underscore> <period> <Return>           : "Ã£"  U0323   # COMBINING DOT BELOW
<Multi_key> <underscore> <quotedbl> <Return>         : "Ã§"  U0324   # COMBINING DIAERESIS BELOW
<Multi_key> <underscore> <o> <Return>                : "Ã•"  U0325   # COMBINING RING BELOW (conflicts with ≈ç, use -o)
<Multi_key> <underscore> <underscore> <comma> <Return> : "Ã¶"  U0326   # COMBINING COMMA BELOW (conflicts with ≈´, use -u)
<Multi_key> <underscore> <comma> <Return>            : "Ãß"  U0327   # COMBINING CEDILLA
<Multi_key> <underscore> <semicolon> <Return>        : "Ã®"  U0328   # COMBINING OGONEK
<Multi_key> <underscore> <bar> <Return>              : "Ã©"  U0329   # COMBINING VERTICAL LINE BELOW
<Multi_key> <underscore> <bracketleft> <Return>      : "Ã™"  U032A   # COMBINING BRIDGE BELOW
<Multi_key> <underscore> <w> <Return>                : "Ã´"  U032B   # COMBINING INVERTED DOUBLE ARCH BELOW
<Multi_key> <underscore> <v> <Return>                : "Ã¨"  U032C   # COMBINING CARON BELOW
<Multi_key> <underscore> <asciicircum> <Return>      : "Ã≠"  U032D   # COMBINING CIRCUMFLEX ACCENT BELOW (conflicts with ¬Ø, use -^)
<Multi_key> <underscore> <u> <Return>                : "ÃÆ"  U032E   # COMBINING BREVE BELOW (conflicts with ≈´, use -u)
<Multi_key> <underscore> <u> <v> <Return>            : "ÃØ"  U032F   # COMBINING INVERTED BREVE BELOW (conflicts with ≈´, use -u)
<Multi_key> <underscore> <asciitilde> <Return>       : "Ã∞"  U0330   # COMBINING TILDE BELOW
<Multi_key> <underscore> <underscore> <Return>       : "Ã±"  U0331   # COMBINING MACRON BELOW (conflicts with ¬Ø, use -^)
<Multi_key> <underscore> <underscore> <underscore> <Return> : "Ã≤"  U0332   # COMBINING LOW LINE (UNDERLINE) (conflicts with ¬Ø, use -^)
<Multi_key> <underscore> <underscore> <equal> <Return> : "Ã≥"  U0333   # COMBINING DOUBLE LOW LINE (UNDERLINE) (conflicts with ¬Ø, use -^)
<Multi_key> <underscore> <bracketright> <Return>     : "Ã∫"  U033A   # COMBINING INVERTED BRIDGE BELOW
<Multi_key> <underscore> <bracketleft> <bracketright> <Return> : "Ãª"  U033B   # COMBINING SQUARE BELOW
<Multi_key> <underscore> <M> <Return>                : "Ãº"  U033C   # COMBINING SEAGULL BELOW
<Multi_key> <underscore> <underscore> <underscore> <equal> <Return> : "Õá"  U0347   # COMBINING EQUALS SIGN BELOW (conflicts with ≈´, use -u)
<Multi_key> <underscore> <bar> <bar> <Return>        : "Õà"  U0348   # COMBINING DOUBLE VERTICAL LINE BELOW
<Multi_key> <underscore> <notsign> <Return>          : "Õâ"  U0349   # COMBINING LEFT ANGLE BELOW
<Multi_key> <parenright> <underscore> <Return>       : "Õú"  U035C   # COMBINING DOUBLE BREVE BELOW

<Multi_key> <minus> <tilde> <Return> : "Ã¥"  U0334   # COMBINING TILDE OVERLAY
<Multi_key> <minus> <minus> <Return> : "Ã∂"  U0336   # COMBINING LONG STROKE OVERLAY (STRIKETHROUGH)
<Multi_key> <slash> <slash> <Return> : "Ã∏"  U0338   # COMBINING LONG SOLIDUS OVERLAY (conflicts with useless \)

