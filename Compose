################################
# (m) International Morse Code #
################################

<Multi_key> <m> <c> <a> : "·-"     # MORSE CODE A
<Multi_key> <m> <c> <b> : "-···"   # MORSE CODE B
<Multi_key> <m> <c> <c> : "-·-·"   # MORSE CODE C
<Multi_key> <m> <c> <d> : "-··"    # MORSE CODE D
<Multi_key> <m> <c> <e> : "·" U00B7  # MORSE CODE E (MIDDLE DOT)
<Multi_key> <m> <c> <f> : "··-·"   # MORSE CODE F
<Multi_key> <m> <c> <g> : "--·"    # MORSE CODE G
<Multi_key> <m> <c> <h> : "····"   # MORSE CODE H
<Multi_key> <m> <c> <i> : "··"     # MORSE CODE I
<Multi_key> <m> <c> <j> : "·---"   # MORSE CODE J
<Multi_key> <m> <c> <k> : "-·-"    # MORSE CODE K
<Multi_key> <m> <c> <l> : "·-··"   # MORSE CODE L
<Multi_key> <m> <c> <m> : "--"     # MORSE CODE M
<Multi_key> <m> <c> <n> : "-·"     # MORSE CODE N
<Multi_key> <m> <c> <o> : "---"    # MORSE CODE O
<Multi_key> <m> <c> <p> : "·--·"   # MORSE CODE P
<Multi_key> <m> <c> <q> : "--·-"   # MORSE CODE Q
<Multi_key> <m> <c> <r> : "·-·"    # MORSE CODE R
<Multi_key> <m> <c> <s> : "···"    # MORSE CODE S
<Multi_key> <m> <c> <t> : "-" U002D  # MORSE CODE T (HYPHEN-MINUS)
<Multi_key> <m> <c> <u> : "··-"    # MORSE CODE U
<Multi_key> <m> <c> <v> : "···-"   # MORSE CODE V
<Multi_key> <m> <c> <w> : "·--"    # MORSE CODE W
<Multi_key> <m> <c> <x> : "-··-"   # MORSE CODE X
<Multi_key> <m> <c> <y> : "-·--"   # MORSE CODE Y
<Multi_key> <m> <c> <z> : "--··"   # MORSE CODE Z

<Multi_key> <m> <x> <grave> <a>       : "·--·-"   # MORSE CODE À
<Multi_key> <m> <x> <o> <a>           : "·--·-"   # MORSE CODE Å
